{
  "request": {
    "method": "POST",
    "url": "/v1/storage/update"
  },

  "response": {
    "status": 200,
    "jsonBody": {
      "responseType": "update",
      "result": "success",
      "storage": {
        "id": "123",
        "title": "{{{jsonPath request.body '$.storage.title'}}}",
        "description": "Хранилище для клиентов с не активной почтой",
        "capacity": "100",
        "availability": "99.99",
        "paymentType": "{{{jsonPath request.body '$.storage.paymentType'}}}",
        "readSpeed": "{{{jsonPath request.body '$.storage.readSpeed'}}}",
        "writeSpeed": "{{{jsonPath request.body '$.storage.writeSpeed'}}}",
        "lock": "123",
        "permissions": ["create"]
      }
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
