{
  "request": {
    "method": "POST",
    "url": "/v2/storage/delete"
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "responseType": "delete",
      "result": "success",
      "storage": {
        "id": "{{{jsonPath request.body '$.ad.id'}}}",
        "title": "Some storage",
        "description": "Хранилище для пользователей без почты",
        "capacity": "100",
        "availability": "99.99",
        "paymentType": "free",
        "readSpeed": "100",
        "writeSpeed": "100",
        "lock": "123",
        "permissions": ["create"],
        "enableOptimize": "0"
      }
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"]
  }
}
